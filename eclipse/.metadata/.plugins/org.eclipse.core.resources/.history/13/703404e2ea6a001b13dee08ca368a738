package ru.GraindGreed.Slots;

import net.minecraft.inventory.IInventory;
import net.minecraft.inventory.Slot;

public class SlotBase extends Slot{

	public SlotBase(IInventory newInventory, int slotID, int posX, int posY) {
		
		super(newInventory, slotID, posX, posY);
		
	}
	
	public boolean canShift() {
		
		return true;
		
	}

}
